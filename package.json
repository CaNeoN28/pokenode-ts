{
  "name": "pokenode",
  "version": "1.0.0",
  "description": "The PokeAPI Node wrapper build in TypeScript",
  "main": "dist/index.js",
  "types": "typings/types.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev -r --respawn --transpile-only --ignore-watch node_modules src/index.ts",
    "start": "node dist/index.js",
    "types": "dts-bundle --removeSource --name pokenode --main ./dist --out ./typings/types.d.ts --baseDir ./dist",
    "docs": "docsify serve ./docs",
    "lint": "eslint src --ext js,ts",
    "lint:fix": "eslint src --ext js,ts --fix",
    "format": "prettier 'src/**/*.[jt]s?(x)' --write",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test": "jest --passWithNoTests",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/node": "^7.13.13",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-typescript": "^7.13.0",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/typescript": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "dts-bundle": "^0.7.3",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.6",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "express": "^4.17.1"
  }
}
