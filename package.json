{
  "name": "pokenode",
  "version": "1.0.0",
  "description": "The PokeAPI Node wrapper build in TypeScript",
  "main": "dist/index.js",
  "types": "@types/index.d.ts",
  "scripts": {
    "build": "tsc",
    "types": "dts-bundle --removeSource --name pokenode --main ./dist --out ./@types/index.d.ts --baseDir ./dist",
    "docs": "docsify serve ./docs",
    "lint": "eslint src --ext js,ts",
    "lint:fix": "eslint src --ext js,ts --fix",
    "format": "prettier 'src/**/*.[jt]s?(x)' --write",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test": "jest --passWithNoTests",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": {
    "name": "Gabb-c",
    "url": "https://github.com/Gabb-c"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.2",
    "@babel/node": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-typescript": "^7.13.0",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/jest": "^26.0.23",
    "@types/typescript": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.23.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "dts-bundle": "^0.7.3",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "ts-jest": "^26.5.6",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "http-status-codes": "^2.1.4",
    "lodash": "^4.17.21"
  }
}
